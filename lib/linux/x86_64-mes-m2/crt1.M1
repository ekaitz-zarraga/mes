:_start
	push___%rbp
	mov____%rsp,%rbp

	;; M2-Planet pushes forward
	mov____%rbp,%rax        ;argc
	add____$i8,%rax !8
	mov____(%rax),%rax
	push___%rax

	mov____%rbp,%rax        ;argv
	add____$i8,%rax !16
	push___%rax

	mov____%rbp,%rax        ;envp
	add____$i8,%rax !8
	mov____(%rax),%rax
	add____$i8,%rax !0x03
	shl____$i8,%rax !0x03
	add____%rbp,%rax
	push___%rax

	call32 %FUNCTION___init_io
	call32 %FUNCTION_main

	mov____%rax,%rdi
	mov____$i32,%rax SYS_exit
	syscall
