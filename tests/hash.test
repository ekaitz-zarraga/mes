(define-module (tests hash)
  #:use-module (mes test))

(cond-expand
 (mes
  (mes-use-module (mes test)))
 (else))

(pass-if "make-hash-table" (make-hash-table))
(pass-if "make-hash-table with size" (make-hash-table 10))

(result 'report)
